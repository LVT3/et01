programa
{
	
	funcao inicio()
	{
		inteiro tamanhoLinha = 0
	
		escreva("digite o tamanaho da linha ")
		leia(tamanhoLinha)
		para (inteiro x = 1; x <= tamanhoLinha; x = x ++  ){
				escreva("-")
		}
		escreva("\naFim de programa")
		
	}
}
